  },
  "homepage": "https://github.com/mapmeld/quantum-quail#readme",
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "@qiskit/cloud": "^0.5.0",
    "@types/request": "^2.48.1"
